<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="#3E109A"
        tools:context=".welcome">

    <ImageView
            android:layout_width="488dp"
            android:layout_height="0dp"
            app:srcCompat="@drawable/a4a6a2fdcc6eea53cb376de55bafea9f_removebg_preview_1"
            android:id="@+id/star"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="3dp" android:layout_marginBottom="279dp"
            app:layout_constraintBottom_toTopOf="@+id/aaa"/>
    <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" app:srcCompat="@drawable/group_44"
            android:id="@+id/imageView2"
            android:layout_marginTop="349dp"
            android:layout_marginBottom="231dp" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintBottom_toTopOf="@+id/aaa" app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintStart_toStartOf="parent"/>
    <Button
            android:text="continuar"
            android:layout_width="129dp"
            android:onClick="aaa"
            android:layout_height="48dp"
            android:id="@+id/aaa"
            android:visibility="visible"
            android:background="#5E17EB"
            tools:ignore="MissingConstraints,OnClick" android:textColor="#FFFFFF"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/imageView2" android:layout_marginBottom="73dp"/>


</android.support.constraint.ConstraintLayout>
